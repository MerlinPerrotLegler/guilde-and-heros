{
  "name": "guildes",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "npx prisma generate && nuxt build",
    "preview": "node -r dotenv/config .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "start": "node -r dotenv/config .output/server/index.mjs",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "dotenv": "^17.3.1",
    "nuxt": "^3.14.0",
    "tailwindcss": "^4.0.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "mysql2": "^3.17.1",
    "prisma": "^6.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
